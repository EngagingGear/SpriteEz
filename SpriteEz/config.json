{
  "img-file": "sprite.png",
  "css-file": "sprite.css",
  "highlight": true,
  "disable": true,
  "hmultiplier": "1.2",
  "dmultiplier": "1.0",
  "ntemplate": ".$$IMAGE-CLASS$$.$$NAME$$, .$$IMAGE-CLASS$$-inline.$$NAME$$,.$$IMAGE-CLASS$$.$$NAME$$$$SUFFIX$$ { $$CSS$$ }",
  "htemplate": ".$$IMAGE-CLASS$$.$$NAME$$:hover, .$$IMAGE-CLASS$$-inline.$$NAME$$:hover,.$$IMAGE-CLASS$$.$$NAME$$$$SUFFIX$$ {$$CSS$$ }",
  "dtemplate": ".$$IMAGE-CLASS$$.$$NAME$$:disabled, .$$IMAGE-CLASS$$-inline.$$NAME$$:disabled, .$$IMAGE-CLASS$$.$$NAME$$$$SUFFIX$$ { $$CSS$$ }",
  "generate-help": true,
  "help-file": "help.html",
  "gray-pixel-threshold": "0.0118",
  "generate-with-filters": false,
  "image-class": "sprite-ez",
  "imgtemplate": ".$$IMAGE-CLASS$$,.$$IMAGE-CLASS$$-inline { background-image: url('./$$FILE$$') !important; background-repeat: no-repeat !important; }\n.$$IMAGE-CLASS$$-inline {display: inline-block;}",
  "normal-css-suffix": "_n",
  "highlighted-css-suffix": "_h",
  "disabled-css-suffix": "_d"
}